{
  "name": "european-alternatives",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate:research": "node scripts/generate-research-catalog.mjs",
    "generate:trust-signals": "node scripts/generate-trust-web-signals.mjs",
    "validate:us-vendors": "node scripts/validate-us-vendor-comparisons.cjs",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "db:export": "tsx scripts/export-to-json.mjs",
    "db:import": "php scripts/db-import.php --source tmp/export/catalog.json",
    "db:migrate": "bash scripts/db-migrate.sh",
    "db:test": "tsx scripts/parity-tests.mjs",
    "db:shadow": "tsx scripts/shadow-diff.mjs",
    "db:snapshot": "tsx scripts/snapshot-baseline.mjs",
    "db:snapshot:compare": "tsx scripts/snapshot-compare.mjs",
    "db:backup": "bash scripts/db-backup.sh",
    "db:restore": "bash scripts/db-restore.sh",
    "db:export-ts": "php scripts/db-export-to-ts.php"
  },
  "dependencies": {
    "@fontsource/oswald": "^5.2.8",
    "@fontsource/roboto": "^5.2.10",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.29.2",
    "i18next": "^25.8.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.39.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.2.0",
    "vitest": "^4.0.18"
  }
}
